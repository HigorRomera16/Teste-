                <output expression="&quot;O primeiro nome digitado foi:&quot; &amp; armnomes" newline="True"/>
                <output expression="&quot;Agora,digite seu sobrenome&quot;" newline="True"/>
                <declare name="armsobrenomes" type="String" array="False" size=""/>
                <input variable="armsobrenomes"/>
                <output expression="&quot;o sobrenome digitado foi:&quot; &amp; armsobrenomes" newline="True"/>
                <output expression="&quot;Em seguida, coloque o seu CPF&quot;" newline="True"/>
                <declare name="armCPF" type="String" array="False" size=""/>
                <input variable="armCPF"/>
                <output expression="&quot;O seu CPF digitado foi:&quot; &amp; armCPF" newline="True"/>
                <output expression="&quot;&#211;timo&quot;" newline="True"/>
                <output expression="&quot;Agora, digite sua idade, por favor&quot;" newline="True"/>
                <declare name="armidade" type="String" array="False" size=""/>
