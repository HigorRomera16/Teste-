                <output expression="&quot;Voc&#234; tem telefone fixo? Responda sim ou n&#227;o&quot;" newline="True"/>
                <declare name="decisao" type="String" array="False" size=""/>
                <input variable="decisao"/>
                <if expression="decisao==&quot;sim&quot;">
