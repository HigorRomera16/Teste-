    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="repeticao" type="Integer" array="False" size=""/>
            <for variable="repeticao" start="1" end="5" direction="inc" step="1">
                <output expression="&quot;cadastro de usu&#225;rio&quot; &amp; &quot; &quot; &amp; &quot; &quot; &amp; repeticao" newline="True"/>
                <output expression="&quot;Ol&#225;! Seja bem-vindo ao sistemas de cadastro de usu&#225;rios&quot;" newline="True"/>
                <output expression="&quot;Por favor digite seu primeiro nome&quot;" newline="True"/>
                <declare name="armnomes" type="String" array="False" size=""/>
