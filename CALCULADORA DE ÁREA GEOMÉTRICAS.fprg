            <declare name="C1" type="Integer" array="False" size=""/>
            <input variable="C1"/>
            <declare name="RESULTADOQUADRADO" type="Integer" array="False" size=""/>
            <assign variable="RESULTADOQUADRADO" expression="Q1*Q1"/>
            <output expression="&quot;A &#225;rea do quadrado &#233;:&quot; &amp; RESULTADOQUADRADO &amp; &quot;.&quot;" newline="True"/>
            <declare name="RESULTADOTRIANGULO" type="Integer" array="False" size=""/>
            <assign variable="RESULTADOTRIANGULO" expression="T1*T1/2"/>
            <output expression="&quot;A &#225;rea do tri&#226;ngulo &#233;:&quot; &amp; RESULTADOTRIANGULO &amp; &quot;.&quot;" newline="True"/>
            <declare name="RESULTADOCUBO" type="Integer" array="False" size=""/>
            <assign variable="RESULTADOCUBO" expression="C1*C1*C1"/>
            <output expression="&quot;A &#225;rea do cubo &#233;:&quot; &amp; RESULTADOCUBO &amp; &quot;.&quot;" newline="True"/>
        </body>
    </function>
