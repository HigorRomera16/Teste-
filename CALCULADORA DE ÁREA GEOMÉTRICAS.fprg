    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <output expression="&quot;Ol&#225;! Sejam bem-vindo ao calculadora de &#225;rea de figuras geom&#233;tricas&quot;" newline="True"/>
            <output expression="&quot;Por favor digite as medidas do quadrado&quot;" newline="True"/>
            <declare name="Q1" type="Integer" array="False" size=""/>
            <input variable="Q1"/>
            <output expression="&quot;Agora, digite as medidas do tri&#226;ngulo&quot;" newline="True"/>
            <declare name="T1" type="Integer" array="False" size=""/>
